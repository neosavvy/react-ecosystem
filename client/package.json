    "react-hot-loader": "1.3.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.1.0",
    "redux-devtools": "2.1.2",
    "run-sequence": "^1.1.4",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.4",
    "webpack": "1.12.0",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  },
  "scripts": {
    "start": "node dev_server.js"
  },
  "babel": {
    "stage": 0,
    "env": {
      "development": {
        "plugins": [
          "react-transform"
        ],
        "extra": {
          "react-transform": {
            "transforms": [
              {
                "transform": "react-transform-hmr",
                "imports": [
                  "react"
                ],
                "locals": [
                  "module"
                ]
              },
              {
                "transform": "react-transform-catch-errors",
                "imports": [
                  "react",
                  "redbox-react"
                ]
              }
            ]
          }
        }
      }
    }
  },
  "keywords": [
    "React",
    "Redux",
    "Webpack",
    "Gulp"
  ],
