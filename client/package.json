{
  "name": "react-ecosystem",
  "version": "1.0.0",
  "description": "React Ecosystem",
  "main": "src/app.js",
  "dependencies": {
    "classnames": "^2.1.3",
    "express": "^4.13.3",
    "history": "^1.9.0",
    "immutable": "3.7.5",
