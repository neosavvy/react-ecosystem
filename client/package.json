    "chai": "^3.3.0",
    "chai-immutable": "^1.3.0",
    "css-loader": "^0.18.0",
    "eslint": "^1.4.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.3.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.1",
    "gulp-blanket-mocha": "0.0.4",
    "gulp-clean": "^0.3.1",
    "gulp-install": "^0.5.0",
